{
  "name": "matman",
  "displayName": "matman",
  "description": "matman 自动化测试辅助",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "publisher": "wangjq4214",
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:matman.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "matman.helloWorld",
        "title": "Hello World"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rollup -c",
    "lint": "eslint src --ext ts",
    "watch": "rollup -c -w",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "rollup": "^2.21.0",
    "typescript": "^3.8.3",
    "vsce": "^1.77.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "gitdiff-parser": "^0.2.2"
  }
}
