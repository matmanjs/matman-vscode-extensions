{
  "name": "matman",
  "displayName": "matman",
  "description": "matman 自动化测试辅助",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "publisher": "wangjq4214",
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "type": "object",
        "title": "MatmanHelper",
        "properties": {
          "dwt.coverage.lcovpath": {
            "type": "string",
            "default": ".dwt_output",
            "markdownDescription": "产物输出路径"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "matman.fullcoverage",
        "title": "Show FullCoverage"
      },
      {
        "command": "matman.incrementcoverage",
        "title": "Show IncrementCoverage"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/chai.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode none --watch",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.2",
    "ts-loader": "^8.0.0",
    "typescript": "^3.8.3",
    "vsce": "^1.77.0",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "dayjs": "^1.8.32",
    "gitdiff-parser": "^0.2.2",
    "gitlog": "^4.0.0",
    "glob": "^7.1.6",
    "lcov-parse": "^1.0.0",
    "xml2js": "^0.4.23"
  }
}
